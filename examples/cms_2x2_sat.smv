    review_0_1 : 0..3;
    review_1_0 : 0..3;
    review_1_1 : 0..3;
    decision_0 : 0..3;
    decision_1 : 0..3;
INIT
    (assigns_0_0 | assigns_0_1) &
    (assigns_1_0 | assigns_1_1) &
    (assigns_0_0 | assigns_1_0) &
    (assigns_0_1 | assigns_1_1) &
    review_0_0 = 0 &
    review_0_1 = 0 &
    review_1_0 = 0 &
    review_1_1 = 0 &
    decision_0 = 0 &
    decision_1 = 0
TRANS
    (next(review_0_0) = review_0_0 & next(review_0_1) = review_0_1 & next(review_1_0) = review_1_0 & next(review_1_1) = review_1_1 & next(decision_0) = decision_0 & next(decision_1) = decision_1) |
    (assigns_0_0 & review_0_0 = 0 & next(review_0_0) != 0 & next(review_0_1) = review_0_1 & next(review_1_0) = review_1_0 & next(review_1_1) = review_1_1 & next(decision_0) = decision_0 & next(decision_1) = decision_1) |
    (assigns_0_1 & review_0_1 = 0 & next(review_0_1) != 0 & next(review_0_0) = review_0_0 & next(review_1_0) = review_1_0 & next(review_1_1) = review_1_1 & next(decision_0) = decision_0 & next(decision_1) = decision_1) |
    (assigns_1_0 & review_1_0 = 0 & next(review_1_0) != 0 & next(review_0_0) = review_0_0 & next(review_0_1) = review_0_1 & next(review_1_1) = review_1_1 & next(decision_0) = decision_0 & next(decision_1) = decision_1) |
    (assigns_1_1 & review_1_1 = 0 & next(review_1_1) != 0 & next(review_0_0) = review_0_0 & next(review_0_1) = review_0_1 & next(review_1_0) = review_1_0 & next(decision_0) = decision_0 & next(decision_1) = decision_1) |
    (decision_0 = 0 & ((assigns_0_0 -> review_0_0 != 0) & (assigns_0_1 -> review_0_1 != 0)) & next(decision_0) != 0 & (next(decision_0) = review_0_0 | next(decision_0) = review_0_1 | next(decision_0) = review_1_0 | next(decision_0) = review_1_1) & next(decision_1) = decision_1 & next(review_0_0) = review_0_0 & next(review_0_1) = review_0_1 & next(review_1_0) = review_1_0 & next(review_1_1) = review_1_1) |
    (decision_1 = 0 & ((assigns_1_0 -> review_1_0 != 0) & (assigns_1_1 -> review_1_1 != 0)) & next(decision_1) != 0 & (next(decision_1) = review_0_0 | next(decision_1) = review_0_1 | next(decision_1) = review_1_0 | next(decision_1) = review_1_1) & next(decision_0) = decision_0 & next(review_0_0) = review_0_0 & next(review_0_1) = review_0_1 & next(review_1_0) = review_1_0 & next(review_1_1) = review_1_1)
HLTLSPEC
    forall A . forall B . exists C .
    ((assigns_0_0[A] = assigns_0_0[B] & assigns_0_1[A] = assigns_0_1[B] & assigns_1_0[A] = assigns_1_0[B] & assigns_1_1[A] = assigns_1_1[B]) & G ((assigns_0_0[A] -> (decision_0[A] = 0 <-> decision_0[B] = 0) & (review_0_0[A] = 0 <-> review_0_0[B] = 0) & (review_0_1[A] = 0 <-> review_0_1[B] = 0)) & (assigns_1_0[A] -> (decision_1[A] = 0 <-> decision_1[B] = 0) & (review_1_0[A] = 0 <-> review_1_0[B] = 0) & (review_1_1[A] = 0 <-> review_1_1[B] = 0))) & F (decision_0[A] != 0 & decision_1[A] != 0))
    ->
    ((assigns_0_0[B] = assigns_0_0[C] & assigns_0_1[B] = assigns_0_1[C] & assigns_1_0[B] = assigns_1_0[C] & assigns_1_1[B] = assigns_1_1[C]) & G ((assigns_0_0[A] -> (decision_0[A] = decision_0[C] & review_0_0[A] = review_0_0[C] & review_0_1[A] = review_0_1[C])) & (assigns_1_0[A] -> (decision_1[A] = decision_1[C] & review_1_0[A] = review_1_0[C] & review_1_1[A] = review_1_1[C]))) & G ((!assigns_0_0[B] -> (review_0_0[B] = review_0_0[C] & review_0_1[B] = review_0_1[C])) & (!assigns_1_0[B] -> (review_1_0[B] = review_1_0[C] & review_1_1[B] = review_1_1[C]))))
